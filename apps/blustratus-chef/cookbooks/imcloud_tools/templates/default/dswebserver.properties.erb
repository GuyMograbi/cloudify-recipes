## If missing, default is 11080
## If set to -1, then http:// port is disabled
# port=

## If missing, default is http port + 1
## If http port is disabled, then default is 11081
## If set to -1, then https:// port is disabled
## Note: at least one of 'port' or 'https.port' must be valid
# https.port=

## If missing, default is http port + 2
## If http port is disabled, then default is 11083
## A valid 'port' is needed.
# status.port=

## This is for console security. With the Liberty profile,
## 'web' equals the default system user created via init.sh/.bat.
## The default is 'web'. The Console Security task can be used to change this.
# auth=metadb|web|disabled

# Disable http
port=-1

# HTTPS
https.port=8443
#status.port=11083

# Authentication
auth=metadb

# Cognos
cognosURL=https://<%= node[:cognos][:ip] %>/ibmcognos
