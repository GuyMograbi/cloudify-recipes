# .bashrc

# Source global definitions
if [ -f /etc/bashrc ]; then
        . /etc/bashrc
fi

# User specific aliases and functions

# The following three lines have been added by UDB DB2.
if [ -f <%= node[:db2][:instance][:home_dir] %>/sqllib/db2profile ]; then
    . <%= node[:db2][:instance][:home_dir] %>/sqllib/db2profile
fi


export CLASSPATH=$CLASSPATH:<%= node[:db2][:instance][:home_dir] %>/sqllib/json/lib/
export PATH=$PATH:<%= node[:db2][:instance][:home_dir] %>/sqllib/json/bin/
